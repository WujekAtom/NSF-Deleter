<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.lotus.com/dxl" name="Configuration" nocompose="true" publicaccess="false" renderpassthrough="true">
  <code event="windowtitle">
    <formula>"Configuration"</formula>
  </code>
  <body>
    <richtext>
      <pardef id="1"/>
      <par def="1"/>
      <par def="1">
        <run>
          <font size="19pt"/>CONFIGURATION</run>
      </par>
      <par def="1">
        <run>
          <font size="12pt"/>
        </run>
      </par>
      <par def="1">
        <run>
          <font size="12pt"/>
        </run>
      </par>
      <table cellbordercolor="silver" widthtype="fixedleft" rowspacing="0.0785in" columnspacing="0.0785in" refwidth="9.7715in">
        <tablecolumn width="2.7035in"/>
        <tablecolumn width="3.2201in"/>
        <tablecolumn width="3.3771in"/>
        <tablerow>
          <tablecell>
            <pardef id="3" keepwithnext="true" keeptogether="true"/>
            <par def="3">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>Server with Domino Directory</run>
            </par>
            <par def="3">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>(names.nsf) to search in:</run>
            </par>
          </tablecell>
          <tablecell>
            <pardef id="4" keepwithnext="true" keeptogether="true"/>
            <par def="4">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
                <field type="text" kind="editable" name="NABserver"/>
              </run>
              <compositedata type="98" prevtype="65418" nexttype="222" afterparcount="7" containertype="65418" aftercontainercount="1" afterbegincount="3">
Yg4BAIQAAAAAAAAAAAA=
</compositedata>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
            </par>
            <par def="4">
              <run>
                <font size="8pt" name="Arial" pitch="variable" truetype="true" familyid="20" color="gray"/>example: APPSERVER01/SRV/ACME</run>
            </par>
          </tablecell>
          <tablecell>
            <pardef id="5" keepwithnext="true" keeptogether="true"/>
            <par def="5">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
            </par>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell valign="center">
            <pardef id="6" keepwithnext="true" keeptogether="true"/>
            <par def="6">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>Delete databases from single server:</run>
            </par>
            <par def="6">
              <run>
                <font size="9pt" name="Arial" pitch="variable" truetype="true" familyid="20" color="gray"/>empty for current server</run>
            </par>
          </tablecell>
          <tablecell valign="center">
            <pardef id="7" keepwithnext="true" keeptogether="true"/>
            <par def="7">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
                <field type="text" kind="editable" name="deleteFromServer"/>
              </run>
              <compositedata type="98" prevtype="65418" nexttype="222" afterparcount="7" containertype="65418" aftercontainercount="1" afterbegincount="3">
Yg4BAIQAAAAAAAAAAAA=
</compositedata>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
            </par>
            <par def="7">
              <run>
                <font size="8pt" name="Arial" pitch="variable" truetype="true" familyid="20" color="gray"/>example: APPSERVER01/SRV/ACME</run>
            </par>
          </tablecell>
          <tablecell>
            <pardef id="8" keepwithnext="true" keeptogether="true"/>
            <par def="8">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>Delete from single server (not clustered)</run>
            </par>
            <par def="8">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
                <field borderstyle="none" lookupeachchar="false" lookupaddressonrefresh="false" type="keyword" kind="editable" name="singleServerDelete">
                  <keywords helperbutton="false" columns="1" ui="radiobutton">
                    <textlist>
                      <text>YES | 1</text>
                      <text>NO | 0</text>
                    </textlist>
                  </keywords>
                  <code event="defaultvalue">
                    <formula>"0"</formula>
                  </code>
                </field>
              </run>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
            </par>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell valign="center">
            <par def="6">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>Delete databases from cluster 1:</run>
            </par>
            <par def="6">
              <run>
                <font size="9pt" name="Arial" pitch="variable" truetype="true" familyid="20" color="gray"/>empty for current server</run>
            </par>
          </tablecell>
          <tablecell valign="center">
            <par def="7">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
                <field type="text" kind="editable" name="serverReplica1"/>
              </run>
              <compositedata type="98" prevtype="65418" nexttype="222" afterparcount="6" containertype="65418" aftercontainercount="1" afterbegincount="3">
Yg4BAIQAAAAAAAAAAAA=
</compositedata>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
            </par>
            <par def="7">
              <run>
                <font size="8pt" name="Arial" pitch="variable" truetype="true" familyid="20" color="gray"/>example: APPSERVER01/SRV/ACME</run>
            </par>
          </tablecell>
          <tablecell>
            <par def="8">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>Delete from cluster1</run>
            </par>
            <pardef id="9" hide="notes" keepwithnext="true" keeptogether="true"/>
            <par def="9">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
                <field borderstyle="none" lookupeachchar="false" lookupaddressonrefresh="false" type="keyword" kind="editable" name="cluster1delete">
                  <keywords helperbutton="false" columns="1" ui="radiobutton">
                    <textlist>
                      <text>YES | 1</text>
                      <text>NO | 0</text>
                    </textlist>
                  </keywords>
                  <code event="defaultvalue">
                    <formula>"1"</formula>
                  </code>
                </field>
              </run>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
            </par>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell valign="center">
            <par def="6">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>Delete databases from cluster 2:</run>
            </par>
            <par def="6">
              <run>
                <font size="9pt" name="Arial" pitch="variable" truetype="true" familyid="20" color="gray"/>empty for current server</run>
            </par>
          </tablecell>
          <tablecell valign="center">
            <par def="7">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
                <field type="text" kind="editable" name="serverReplica2"/>
              </run>
              <compositedata type="98" prevtype="65418" nexttype="222" afterparcount="6" containertype="65418" aftercontainercount="1" afterbegincount="3">
Yg4BAIQAAAAAAAAAAAA=
</compositedata>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
            </par>
            <par def="7">
              <run>
                <font size="8pt" name="Arial" pitch="variable" truetype="true" familyid="20" color="gray"/>example: APPSERVER01/SRV/ACME</run>
            </par>
          </tablecell>
          <tablecell>
            <par def="8">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>Delete from cluster2</run>
            </par>
            <par def="9">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
                <field borderstyle="none" lookupeachchar="false" lookupaddressonrefresh="false" type="keyword" kind="editable" name="cluster2delete">
                  <keywords helperbutton="false" columns="1" ui="radiobutton">
                    <textlist>
                      <text>YES | 1</text>
                      <text>NO | 0</text>
                    </textlist>
                  </keywords>
                  <code event="defaultvalue">
                    <formula>"1"</formula>
                  </code>
                </field>
              </run>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
            </par>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell>
            <par def="6">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>Default prefix for databases (path)</run>
            </par>
            <par def="6">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>reads from file</run>
            </par>
            <par def="6">
              <run>
                <font size="9pt" name="Arial" pitch="variable" truetype="true" familyid="20" color="gray"/>for example: (if empty - read from FILE!) </run>
            </par>
            <par def="6">
              <run>
                <font size="9pt" name="Arial" pitch="variable" truetype="true" familyid="20" color="gray"/>mail/</run>
            </par>
            <par def="6">
              <run>
                <font size="9pt" name="Arial" pitch="variable" truetype="true" familyid="20" color="gray"/>archive/</run>
            </par>
            <par def="6">
              <run>
                <font size="9pt" name="Arial" pitch="variable" truetype="true" familyid="20" color="gray"/>folder/</run>
            </par>
          </tablecell>
          <tablecell>
            <par def="7">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
                <field type="text" kind="editable" name="prefix"/>
              </run>
              <compositedata type="98" prevtype="65418" nexttype="222" afterparcount="6" containertype="65418" aftercontainercount="1" afterbegincount="3">
Yg4BAIQAAAAAAAAAAAA=
</compositedata>
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
            </par>
          </tablecell>
          <tablecell>
            <par def="8">
              <run>
                <font size="12pt" style="bold" name="Arial" pitch="variable" truetype="true" familyid="20" color="red"/>Leave empty if you provide path in file!</run>
            </par>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell>
            <par def="6">
              <run>
                <font size="12pt" style="strikethrough" name="Arial" pitch="variable" truetype="true" familyid="20"/>Mail server to EXCLUDE</run>
            </par>
            <par def="6">
              <run>
                <font size="9pt" style="strikethrough" name="Arial" pitch="variable" truetype="true" familyid="20" color="gray"/>Person docs contains this server will not be processed</run>
            </par>
          </tablecell>
          <tablecell>
            <par def="7">
              <run>
                <font size="12pt" style="strikethrough" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
              <run>
                <font size="12pt" style="strikethrough" name="Arial" pitch="variable" truetype="true" familyid="20"/>
                <field type="text" kind="editable" name="excludeServer"/>
              </run>
              <compositedata type="98" prevtype="65418" nexttype="222" afterparcount="6" containertype="65418" aftercontainercount="1" afterbegincount="3">
Yg4BAIQAAAAAAAAAAAA=
</compositedata>
              <run>
                <font size="12pt" style="strikethrough" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
            </par>
          </tablecell>
          <tablecell>
            <par def="8">
              <run>
                <font size="12pt" name="Arial" pitch="variable" truetype="true" familyid="20"/>
              </run>
            </par>
          </tablecell>
        </tablerow>
      </table>
      <par def="1">
        <run>
          <font size="12pt"/>
        </run>
      </par>
    </richtext>
  </body>
</form>